<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
   xmlns:g="urn:import:com.google.gwt.user.client.ui">
   
   <ui:with field="consts" type="com.futmesa.client.module.config.ConfigModuleConsts" />

   <ui:style src="championshipconfigviewport.css" />
   
   <g:VerticalPanel ui:field='panel'>
      <g:SimplePanel ui:field='championshipPanel' addStyleNames='{style.tablePanel}'></g:SimplePanel>
      <g:HorizontalPanel ui:field='btnPanel'>
         <g:Button addStyleNames='{style.customBtn}' ui:field='addBtn' text="{consts.addBtn}"></g:Button>
         <g:Button addStyleNames='{style.customBtn}' ui:field='cancelBtn' text="{consts.cancelBtn}"></g:Button>
         <g:Button addStyleNames='{style.customBtn}' ui:field='saveBtn' text="{consts.saveBtn}"></g:Button>
      </g:HorizontalPanel> 
   </g:VerticalPanel>
</ui:UiBinder>