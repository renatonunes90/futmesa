<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
  xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:c="urn:import:com.google.gwt.user.cellview.client">

  <ui:with field="consts" type="com.futmesa.client.module.main.dialogs.results.ResultsDialogConsts" /> 
  	
  <ui:style src="resultsdialog.css" /> 

  <g:VerticalPanel ui:field='panel'>
     <c:CellTable ui:field='gameTable' addStyleNames='{style.resultGameTable}'></c:CellTable>
     <g:HorizontalPanel ui:field='btnPanel'>
	     <g:Button ui:field='cancelBtn' addStyleNames='{style.cancelBtn}' text='{consts.cancelBtn}'></g:Button>
	   	 <g:Button ui:field='insertResultBtn' addStyleNames='{style.insertResultBtn}' text='{consts.saveBtn}'></g:Button>
   	 </g:HorizontalPanel>
  </g:VerticalPanel>
</ui:UiBinder>