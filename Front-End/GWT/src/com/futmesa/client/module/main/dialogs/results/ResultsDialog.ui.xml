<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
  xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:c="urn:import:com.google.gwt.user.cellview.client">

  <ui:with field="consts" type="com.futmesa.client.module.main.dialogs.results.ResultsDialogConsts" /> 
  	
  <ui:style src="resultsdialog.css" /> 

  <g:VerticalPanel ui:field='panel' addStyleNames='{style.customPanel}'>
     <g:Label addStyleNames='{style.classificationTitle}' text="{consts.insertingResultsLabel}"></g:Label>
     <g:SimplePanel ui:field='gameTablePanel'></g:SimplePanel>
   	 <g:Button ui:field='insertResultBtn' addStyleNames='{style.insertResultBtn}' text='Salvar Resultados'></g:Button>
  </g:VerticalPanel>
</ui:UiBinder>