<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
  xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:c="urn:import:com.google.gwt.user.cellview.client">

  <ui:style src="classificationviewport.css" /> 

  <g:HorizontalPanel ui:field='panel'>
    <g:VerticalPanel>
     	<g:Label addStyleNames='{style.classificationTitle}'>Tabela</g:Label>
     	<c:CellTable addStyleNames='{style.cellTable}'  ui:field='cellTable' />
     </g:VerticalPanel>
     <g:VerticalPanel>
        <g:Label addStyleNames='{style.classificationTitle}'>Jogos</g:Label>
     </g:VerticalPanel>
  </g:HorizontalPanel>
</ui:UiBinder>