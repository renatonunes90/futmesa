<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
  xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:c="urn:import:com.google.gwt.user.cellview.client"
  xmlns:b="urn:import:com.github.gwtbootstrap.client.ui">

  <ui:with field="consts" type="com.futmesa.client.module.main.viewport.championship.ChampionshipViewportConsts" /> 
  	
  <ui:style src="classificatorydeathmatchviewport.css" /> 

  <b:FluidContainer ui:field='panel'>
	  <b:FluidRow>
	  	<b:Column size="1"></b:Column>
	  	<b:Column size="10">
	  			<b:Row> </b:Row>
	  			<b:Row>
	  				<b:Column size="8" ui:field='leftPanel' addStyleNames='{style.custom-left-panel}'>
	  					<g:Label addStyleNames='{style.classification-title}' text='{consts.classificationTitle}'></g:Label>
	  				</b:Column>
	  				<b:Column size="4" ui:field='rightPanel' addStyleNames='{style.custom-right-panel}'>
	  				     <g:Label addStyleNames='{style.classification-title}' text='{consts.gamesTitle}'></g:Label>
				         <g:SimplePanel ui:field='gameTablePanel'></g:SimplePanel>
	  				</b:Column>
	  			</b:Row>
	  	</b:Column>
	  	<b:Column size="1"></b:Column>
	  </b:FluidRow>
  </b:FluidContainer>
  <!--  <g:HorizontalPanel ui:field='panel'>
    <g:VerticalPanel ui:field='leftPanel' addStyleNames='{style.customLeftPanel}'>
     	<g:Label addStyleNames='{style.classificationTitle}' text='{consts.classificationTitle}'></g:Label>
     </g:VerticalPanel>
     <g:VerticalPanel ui:field='rightPanel' addStyleNames='{style.customRightPanel}'>
        <g:Label addStyleNames='{style.classificationTitle}' text='{consts.gamesTitle}'></g:Label>
        <g:SimplePanel ui:field='gameTablePanel'></g:SimplePanel>
     </g:VerticalPanel>
  </g:HorizontalPanel> -->
</ui:UiBinder>