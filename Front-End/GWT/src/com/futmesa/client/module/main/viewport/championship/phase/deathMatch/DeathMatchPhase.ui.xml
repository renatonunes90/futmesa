<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:com.google.gwt.user.cellview.client">

	<ui:with field="consts"
		type="com.futmesa.client.module.main.viewport.championship.ChampionshipViewportConsts" />

	<ui:style src="deathmatchphase.css" />
	
	<g:HorizontalPanel ui:field='deathMatchPhasePanel'>
	
		<g:HorizontalPanel ui:field='leftPanel' addStyleNames='{style.custom-left-panel}'>
			<g:VerticalPanel>
				<g:HorizontalPanel addStyleNames='{style.player-spot}'>
					<g:Label addStyleNames='{style.player-label} text-left' text='Player 1'></g:Label>
					<g:Label addStyleNames='{style.player-label} text-right' text='1'></g:Label>		
				</g:HorizontalPanel>
				<g:HorizontalPanel addStyleNames='{style.player-spot}'>
					<g:Label addStyleNames='{style.player-label} text-left' text='Player 2'></g:Label>
					<g:Label addStyleNames='{style.player-label} text-right' text='2'></g:Label>		
				</g:HorizontalPanel>
				<g:HorizontalPanel addStyleNames='{style.player-spot}'>
					<g:Label addStyleNames='{style.player-label} text-left' text='Player 3'></g:Label>
					<g:Label addStyleNames='{style.player-label} text-right' text='1'></g:Label>		
				</g:HorizontalPanel>
				<g:HorizontalPanel addStyleNames='{style.player-spot}'>
					<g:Label addStyleNames='{style.player-label} text-left' text='Player 4'></g:Label>
					<g:Label addStyleNames='{style.player-label} text-right' text='0'></g:Label>		
				</g:HorizontalPanel>
			</g:VerticalPanel>
			<g:VerticalPanel addStyleNames='{style.semi-panel}'>
				<g:HorizontalPanel addStyleNames='{style.player-spot}'>
					<g:Label addStyleNames='{style.player-label} text-left' text='Player 2'></g:Label>
					<g:Label addStyleNames='{style.player-label} text-right' text='1'></g:Label>		
				</g:HorizontalPanel>
				<g:HorizontalPanel addStyleNames='{style.player-spot} {style.semi-down}'>
					<g:Label addStyleNames='{style.player-label} text-left' text='Player 3'></g:Label>
					<g:Label addStyleNames='{style.player-label} text-right' text='0'></g:Label>		
				</g:HorizontalPanel>
			</g:VerticalPanel>
			<g:VerticalPanel addStyleNames='{style.final-panel}'>
				<g:HorizontalPanel addStyleNames='{style.player-spot}'>
					<g:Label addStyleNames='{style.player-label} text-left' text='Player 2'></g:Label>
					<g:Label addStyleNames='{style.player-label} text-right' text='1'></g:Label>		
				</g:HorizontalPanel>
			</g:VerticalPanel>
		</g:HorizontalPanel>
		<g:HorizontalPanel ui:field='rightPanel' addStyleNames='{style.custom-right-panel}'>
			<g:VerticalPanel addStyleNames='{style.final-panel}'>
				<g:HorizontalPanel addStyleNames='{style.player-spot}'>
					<g:Label addStyleNames='{style.player-label} text-left' text='0'></g:Label>		
					<g:Label addStyleNames='{style.player-label} text-right' text='Player 5'></g:Label>		
				</g:HorizontalPanel>
			</g:VerticalPanel>
			<g:VerticalPanel addStyleNames='{style.semi-panel}'>
				<g:HorizontalPanel addStyleNames='{style.player-spot}'>
					<g:Label addStyleNames='{style.player-label} text-left' text='1'></g:Label>		
					<g:Label addStyleNames='{style.player-label} text-right' text='Player 5'></g:Label>		
				</g:HorizontalPanel>
				<g:HorizontalPanel addStyleNames='{style.player-spot} {style.semi-down}'>
					<g:Label addStyleNames='{style.player-label} text-left' text='0'></g:Label>		
					<g:Label addStyleNames='{style.player-label} text-right' text='Player 8'></g:Label>		
				</g:HorizontalPanel>
			</g:VerticalPanel>
			<g:VerticalPanel>
				<g:HorizontalPanel addStyleNames='{style.player-spot}'>
					<g:Label addStyleNames='{style.player-label} text-left' text='1'></g:Label>		
					<g:Label addStyleNames='{style.player-label} text-right' text='Player 5'></g:Label>
				</g:HorizontalPanel>
				<g:HorizontalPanel addStyleNames='{style.player-spot}'>
					<g:Label addStyleNames='{style.player-label} text-left' text='0'></g:Label>		
					<g:Label addStyleNames='{style.player-label} text-right' text='Player 5'></g:Label>		
				</g:HorizontalPanel>
				<g:HorizontalPanel addStyleNames='{style.player-spot}'>
					<g:Label addStyleNames='{style.player-label} text-left' text='1'></g:Label>		
					<g:Label addStyleNames='{style.player-label} text-right' text='Player 7'></g:Label>	
				</g:HorizontalPanel>
				<g:HorizontalPanel addStyleNames='{style.player-spot}'>
					<g:Label addStyleNames='{style.player-label} text-left' text='2'></g:Label>		
					<g:Label addStyleNames='{style.player-label} text-right' text='Player 8'></g:Label>	
				</g:HorizontalPanel>
			</g:VerticalPanel>
		</g:HorizontalPanel>
	</g:HorizontalPanel>

</ui:UiBinder>