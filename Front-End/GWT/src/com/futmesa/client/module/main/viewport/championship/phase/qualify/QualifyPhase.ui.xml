<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:com.google.gwt.user.cellview.client">

	<ui:with field="consts"
		type="com.futmesa.client.module.main.viewport.championship.ChampionshipViewportConsts" />

	<ui:style src="qualifyphase.css" />
	
	<g:HorizontalPanel ui:field='qualifyPhasePanel' addStyleNames='{style.qualify-panel}'>
		<g:VerticalPanel ui:field='leftPanel' addStyleNames='{style.custom-left-panel}'>
			<g:Label addStyleNames='{style.classification-title}' text='{consts.classificationTitle}'></g:Label>
		</g:VerticalPanel>
		<g:VerticalPanel ui:field='rightPanel' addStyleNames='{style.custom-right-panel}'>
			<g:Label addStyleNames='{style.classification-title}' text='{consts.gamesTitle}'></g:Label>
			<g:SimplePanel ui:field='gameTablePanel'></g:SimplePanel>
		</g:VerticalPanel>
	</g:HorizontalPanel>

</ui:UiBinder>