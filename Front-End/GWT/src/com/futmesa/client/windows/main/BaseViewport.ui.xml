<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" >

 	<ui:style src="baseviewport.css" /> 
	
  	<ui:with field="consts" type="com.futmesa.client.windows.main.BaseViewportConsts" /> 

    <g:DockLayoutPanel unit="EM" ui:field='vLayout'>
      <g:north size="6">
         <g:HorizontalPanel width="100%" height="120px" styleName="{style.nav-bar}" > </g:HorizontalPanel>
      </g:north>

      <g:center>
         <g:HTMLPanel width="100%" height="400px" styleName="{style.cp-content}" ui:field="cpContent">ALGO </g:HTMLPanel>
      </g:center>

      <g:south size="3">
		<g:HorizontalPanel width="100%" height="60px" styleName="{style.nav-bar}"> </g:HorizontalPanel>
       </g:south>
    </g:DockLayoutPanel>

</ui:UiBinder> 