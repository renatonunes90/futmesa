<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
  xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui">

  <ui:with field="appConsts" type="com.futmesa.client.FutMesaConsts" /> 
  <ui:with field="consts" type="com.futmesa.client.windows.main.about.AboutDialogConsts" /> 
  	
  <ui:style src="aboutdialog.css" /> 

  <g:VerticalPanel ui:field='panel' addStyleNames='{style.about-dialog}'>
     <g:Label addStyleNames='{style.app-name}' text='{appConsts.appName}'></g:Label> 
     <g:Label text='{consts.aboutLabel}'></g:Label> 
     <g:Label ui:field='versionPanel' addStyleNames='{style.version-label}' text='{consts.versionLabel}{consts.versionNumber}'></g:Label> 
     <g:Label addStyleNames='{style.copyright}' text='{consts.copyright}'></g:Label> 
     <g:Button ui:field='closeBtn' addStyleNames='{style.close-btn}' text='{consts.closeBtn}'></g:Button>
  </g:VerticalPanel>
</ui:UiBinder>